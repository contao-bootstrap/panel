<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="ContaoBootstrap\Panel\Components\ContentElement\PanelGroupStartElementController">
            <argument type="service" id="netzmacht.contao_toolkit.template_renderer"/>
            <argument type="service" id="netzmacht.contao_toolkit.routing.scope_matcher"/>
            <argument type="service" id="netzmacht.contao_toolkit.response_tagger"/>
            <argument type="service" id="contao.security.token_checker"/>
            <argument type="service" id="contao_bootstrap.core.helper.color_rotate"/>
            <argument type="service" id="netzmacht.contao_toolkit.repository_manager"/>
        </service>

        <service id="ContaoBootstrap\Panel\Components\ContentElement\PanelGroupEndElementController">
            <argument type="service" id="netzmacht.contao_toolkit.template_renderer"/>
            <argument type="service" id="netzmacht.contao_toolkit.routing.scope_matcher"/>
            <argument type="service" id="netzmacht.contao_toolkit.response_tagger"/>
            <argument type="service" id="contao.security.token_checker"/>
            <argument type="service" id="contao_bootstrap.core.helper.color_rotate"/>
            <argument type="service" id="netzmacht.contao_toolkit.repository_manager"/>
        </service>

        <service id="ContaoBootstrap\Panel\Components\ContentElement\PanelSingleElementController">
            <argument type="service" id="netzmacht.contao_toolkit.template_renderer"/>
            <argument type="service" id="netzmacht.contao_toolkit.routing.scope_matcher"/>
            <argument type="service" id="netzmacht.contao_toolkit.response_tagger"/>
            <argument type="service" id="contao.security.token_checker"/>
            <argument type="service" id="contao_bootstrap.core.helper.color_rotate"/>
            <argument type="service" id="netzmacht.contao_toolkit.repository_manager"/>
            <argument>%kernel.project_dir%</argument>
        </service>

        <service id="ContaoBootstrap\Panel\Components\ContentElement\PanelStartElementController">
            <argument type="service" id="netzmacht.contao_toolkit.template_renderer"/>
            <argument type="service" id="netzmacht.contao_toolkit.routing.scope_matcher"/>
            <argument type="service" id="netzmacht.contao_toolkit.response_tagger"/>
            <argument type="service" id="contao.security.token_checker"/>
            <argument type="service" id="contao_bootstrap.core.helper.color_rotate"/>
            <argument type="service" id="netzmacht.contao_toolkit.repository_manager"/>
        </service>

        <service id="ContaoBootstrap\Panel\Components\ContentElement\PanelEndElementController">
            <argument type="service" id="netzmacht.contao_toolkit.template_renderer"/>
            <argument type="service" id="netzmacht.contao_toolkit.routing.scope_matcher"/>
            <argument type="service" id="netzmacht.contao_toolkit.response_tagger"/>
            <argument type="service" id="contao.security.token_checker"/>
            <argument type="service" id="contao_bootstrap.core.helper.color_rotate"/>
            <argument type="service" id="netzmacht.contao_toolkit.repository_manager"/>
            <argument>%kernel.project_dir%</argument>
        </service>
    </services>
</container>
